datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model OptionPosition {
  id           Int       @id @default(autoincrement())
  tweet_id     String    @unique
  ticker       String
  expiry       String
  option_type  String
  strike_price String
  full_tweet   String
  tweeted_at   DateTime?
  created_at   DateTime  @default(now())
}
